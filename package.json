    "tsc:w": "tsc -w"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.2",
    "@parcel/transformer-sass": "2.8.0",
    "autoprefixer": "10.4.13",
    "handlebars": "4.7.7",
    "cssnext": "0.0.0",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "parcel": "2.0.1",
    "postcss": "8.4.19",
    "precss": "3.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "express": "4.18.2"
  },
  "targets": {
    "browserModern": {
      "engines": {
