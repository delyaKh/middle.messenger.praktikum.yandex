{
  "name": "messanger",
  "version": "1.0.0",
  "description": "messanger project for yandex.praktikum",
  "scripts": {
    "start": "parcel build ./src/index.html && node server.js",
    "dev": "parcel ./static/pages/*.html --port 3000 --no-cache --open",
